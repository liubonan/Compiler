big gcd(big a, big b){
  bool c;
  big temp;
  small x;
  x = 1;
  c =true;
  if(a>b){
     temp = a;
	 a=b;
	 b=temp;
  };
  for(c=true;c == true;x=x+1){
   if(a%b==0.00){
      c=false;
      return temp;
    };
    temp=a%b;
    a=b;
    b=temp;  
  };
  return temp;
}

